{
  "name": "hono-open-api-starter",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
